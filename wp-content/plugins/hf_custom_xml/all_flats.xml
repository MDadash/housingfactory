u0434, \u0443\u043b\u0438\u0446\u0430 \u0420\u0430\u0431\u043e\u0447\u0435 - \u041a\u0440\u0435\u0441\u0442\u044c\u044f\u043d\u0441\u043a\u0430\u044f, \u0434\u043e\u043c 57. <br><br>\u041f\u0420\u0415\u041a\u0420\u0410\u0421\u041d\u041e\u0415 \u041c\u0415\u0421\u0422\u041e \u0414\u041b\u042f \u041f\u0420\u041e\u0416\u0418\u0412\u0410\u041d\u0418\u042f \u043f\u043e\u0437\u0432\u043e\u043b\u0438\u0442 \u0412\u0430\u043c \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u043f\u0440\u0435\u0438\u043c\u0443\u0449\u0435\u0441\u0442\u0432\u0430\u043c\u0438 \u0440\u0430\u0437\u0432\u0438\u0442\u043e\u0439 \u0438\u043d\u0444\u0440\u0430\u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u044b \u2013 \u0432 \u0448\u0430\u0433\u043e\u0432\u043e\u0439 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u043e\u0441\u0442\u0438 \u043c\u0430\u0433\u0430\u0437\u0438\u043d\u044b \u041f\u044f\u0442\u0435\u0440\u043e\u0447\u043a\u0430, \u041c\u0430\u043d, \u0433\u0438\u043c\u043d\u0430\u0437\u0438\u044f \u2116 4, \u0434\u0435\u0442\u0441\u043a\u0438\u0435 \u0441\u0430\u0434\u044b \u2116 234, 45, \u0434\u0435\u0442\u0441\u043a\u0438\u0439 \u0441\u0430\u0434 \"\u0421\u0442\u0443\u043f\u0435\u043d\u044c\u043a\u0438\", \u0430\u043f\u0442\u0435\u043a\u0430 \"\u0424\u0430\u0440\u043c\u0413\u0440\u0430\u0434\", \u0434\u0435\u0442\u0441\u043a\u0430\u044f \u043f\u043e\u043b\u0438\u043a\u043b\u0438\u043d\u0438\u043a\u0430 \u2116 6.  <br><br>\u0422\u0420\u0410\u041d\u0421\u041f\u041e\u0420\u0422\u041d\u0410\u042f \u0414\u041e\u0421\u0422\u0423\u041f\u041d\u041e\u0421\u0422\u042c: \u043e\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0438 \u043c\u0430\u0440\u0448\u0440\u0443\u0442\u043d\u044b\u0445 \u0442\u0440\u0430\u043d\u0441\u043f\u043e\u0440\u0442\u043d\u044b\u0445 \u0441\u0440\u0435\u0434\u0441\u0442\u0432 \"\u0422\u042e\u0417\", \"\u043f\u0430\u043c\u044f\u0442\u043d\u0438\u043a \u0421\u0435\u0432\u0435\u0440\u043e\u043c\u043e\u0440\u0446\u0430\u043c\". <br><br>\u0423\u0414\u0410\u0427\u041d\u0410\u042f \u041f\u041b\u0410\u041d\u0418\u0420\u041e\u0412\u041a\u0410:<br><br>\u043e\u0431\u0449\u0430\u044f \u043f\u043b\u043e\u0449\u0430\u0434\u044c \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u044b - (59,5 \u043c), <br>\u0436\u0438\u043b\u0430\u044f - (44,8 \u043c), <br>\u043a\u0443\u0445\u043d\u044f - (6,2 \u043c). <br>\u0441\u0430\u043d\u0443\u0437\u0435\u043b \u0441\u043e\u0432\u043c\u0435\u0449\u0435\u043d\u043d\u044b\u0439, <br>\u0431\u0430\u043b\u043a\u043e\u043d \u043d\u0435 \u043e\u0441\u0442\u0435\u043a\u043b\u0435\u043d.<br><br>\u0420\u0415\u041c\u041e\u041d\u0422: \u041a\u0432\u0430\u0440\u0442\u0438\u0440\u0430 \u0432 \u043e\u0442\u043b\u0438\u0447\u043d\u043e\u043c \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u0438. \u0421\u0434\u0435\u043b\u0430\u043d \u0445\u043e\u0440\u043e\u0448\u0438\u0439 \u0440\u0435\u043c\u043e\u043d\u0442 \u0441\u043e \u0437\u0432\u0443\u043a\u043e\u0438\u0437\u043e\u043b\u044f\u0446\u0438\u0435\u0439 \u043f\u043e \u0432\u0441\u0435\u0439 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0435, \u043d\u0430 \u043f\u043e\u043b\u0443 \u0435\u0432\u0440\u043e\u043f\u0435\u0439\u0441\u043a\u0438\u0435 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u043d\u044b\u0435 (\u043d\u0435 \u043f\u043e\u0434\u0434\u0430\u044e\u0449\u0438\u0435\u0441\u044f \u0434\u0435\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438) \u043f\u043b\u0438\u0442\u043a\u0430 \u0438 \u043b\u0430\u043c\u0438\u043d\u0430\u0442, \u043d\u0430\u0442\u044f\u0436\u043d\u043e\u0439 \u043f\u043e\u0442\u043e\u043b\u043e\u043a \u0441 \u0434\u0438\u0437\u0430\u0439\u043d\u0435\u0440\u0441\u043a\u0438\u043c\u0438 \u0445\u0440\u0443\u0441\u0442\u0430\u043b\u044c\u043d\u044b\u043c\u0438 \u043b\u044e\u0441\u0442\u0440\u0430\u043c\u0438. \u0412 \u0432\u0430\u043d\u043d\u043e\u0439 \u043a\u043e\u043c\u043d\u0430\u0442\u0435 \u043d\u043e\u0432\u0430\u044f \u0441\u0430\u043d\u0442\u0435\u0445\u043d\u0438\u043a\u0430. \u0412 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0435 \u0431\u044b\u043b\u0438 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u044b: \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u0430\u044f \u043c\u0435\u0434\u043d\u0430\u044f \u043f\u0440\u043e\u0432\u043e\u0434\u043a\u0430, \u043e\u0442\u043e\u043f\u0438\u0442\u0435\u043b\u044c\u043d\u0430\u044f \u0441\u0438\u0441\u0442\u0435\u043c\u0430 (\u0431\u0430\u0442\u0430\u0440\u0435\u0438), \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u044b \u0432\u0441\u0435 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b\u0435 \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0435 \u0441\u0447\u0435\u0442\u0447\u0438\u043a\u0438, \u043c\u0435\u0442\u0430\u043b\u043b\u043e\u043f\u043b\u0430\u0441\u0442\u0438\u043a\u043e\u0432\u044b\u0435 \u043e\u043a\u043e\u043d\u043d\u044b\u0435 \u043f\u0430\u043a\u0435\u0442\u044b. \u041a\u0443\u0445\u043d\u044f \u043e\u0431\u043e\u0440\u0443\u0434\u043e\u0432\u0430\u043d\u0430 \u0438\u0434\u0435\u0430\u043b\u044c\u043d\u044b\u043c, \u0434\u043b\u044f \u0445\u043e\u0437\u044f\u044e\u0448\u0435\u043a, \u043a\u0443\u0445\u043e\u043d\u043d\u044b\u043c \u0433\u0430\u0440\u043d\u0438\u0442\u0443\u0440\u043e\u043c \u043e\u0442 \u0434\u0438\u0437\u0430\u0439\u043d\u0435\u0440\u0441\u043a\u043e\u0439 \u0441\u0442\u0443\u0434\u0438\u0438 \"\u041a\u0443\u0445\u043d\u0438 \u041c\u0430\u0440\u0438\u0438\". \u041a\u0432\u0430\u0440\u0442\u0438\u0440\u0430 \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e \u043f\u043e\u0434\u043a\u043b\u044e\u0447\u0435\u043d\u0430 \u043a \u0442\u0435\u043b\u0435\u0432\u0438\u0434\u0435\u043d\u0438\u044e, \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442\u0443.  <br><br>\u0423\u0425\u041e\u0416\u0415\u041d\u041d\u042b\u0419 \u0414\u0412\u041e\u0420: \u0432\u043e \u0434\u0432\u043e\u0440\u0435 \u0436\u0438\u043b\u043e\u0433\u043e \u0434\u043e\u043c\u0430 \u043d\u0430\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u0434\u0435\u0442\u0441\u043a\u0438\u0439 \u0441\u0430\u0434 \"\u0421\u0442\u0443\u043f\u0435\u043d\u044c\u043a\u0438\", \u0430 \u0442\u0430\u043a \u0436\u0435 \u0434\u0435\u0442\u0441\u043a\u0438\u0435 \u0438\u0433\u0440\u043e\u0432\u044b\u0435 \u044d\u043b\u0435\u043c\u0435\u043d\u0442\u044b. \u0414\u043e\u0440 \u0438 \u043f\u043e\u0434\u044a\u0435\u0437\u0434\u043d\u0430\u044f \u0437\u043e\u043d\u0430 \u0437\u0430\u0441\u0430\u0436\u0435\u043d\u0430 \u0434\u0435\u0440\u0435\u0432\u044c\u044f\u043c\u0438 \u0438 \u043a\u0443\u0441\u0442\u0430\u0440\u043d\u0438\u043a\u0430\u043c\u0438. <br><br>\u041a\u0420\u0410\u0421\u0418\u0412\u042b\u0419 \u0414\u041e\u041c: \u043a\u0438\u0440\u043f\u0438\u0447\u043d\u044b\u0439, \u0442\u0435\u043f\u043b\u044b\u0439, 1962 \u0433\u043e\u0434\u0430 \u043f\u043e\u0441\u0442\u0440\u043e\u0439\u043a\u0438, \u0435\u0441\u0442\u044c \u0434\u0432\u043e\u0440\u043e\u0432\u043e\u0435 \u0438 \u043f\u043e\u0434\u044a\u0435\u0437\u0434\u043d\u043e\u0435 \u043e\u0441\u0432\u0435\u0449\u0435\u043d\u0438\u0435, \u0441\u0432\u0435\u0436\u0438\u0439 \u0440\u0435\u043c\u043e\u043d\u0442 \u0432 \u043f\u0430\u0440\u0430\u0434\u043d\u043e\u043c (\u0443\u0445\u043e\u0436\u0435\u043d\u043d\u044b\u0439, \u0447\u0438\u0441\u0442\u044b\u0439 \u043f\u043e\u0434\u044a\u0435\u0437\u0434). <br><br>\u041f\u0420\u0418 \u041f\u0420\u041e\u0414\u0410\u0416\u0415 \u041e\u0421\u0422\u0410\u0415\u0422\u0421\u042f: \u0432\u0441\u044f \u043c\u0435\u0431\u0435\u043b\u044c \u0438 \u0442\u0435\u0445\u043d\u0438\u043a\u0430, \u043a\u0443\u0445\u043e\u043d\u043d\u044b\u0439 \u0433\u0430\u0440\u043d\u0438\u0442\u0443\u0440 \u0432\u043c\u0435\u0441\u0442\u0435 \u0441 \u0434\u0443\u0445\u043e\u0432\u044b\u043c \u0448\u043a\u0430\u0444\u043e\u043c, \u0441\u0442\u0438\u0440\u0430\u043b\u044c\u043d\u0430\u044f \u043c\u0430\u0448\u0438\u043d\u0430, \u0441\u043f\u043b\u0438\u0442 \u0441\u0438\u0441\u0442\u0435\u043c\u0430, \u0432\u0441\u044f \u043c\u0435\u0431\u0435\u043b\u044c (\u043a\u0440\u043e\u0432\u0430\u0442\u044c, \u0434\u0438\u0432\u0430\u043d, \u0441\u0442\u043e\u043b). <br><br>\u0423\u0421\u041b\u041e\u0412\u0418\u042f \u041f\u0420\u041e\u0414\u0410\u0416\u0418: \u043e\u0434\u0438\u043d \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u0438\u043a, \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0430 \u043d\u0430\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u0432 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u043c\u0435\u043d\u0435\u0435 5 \u043b\u0435\u0442. \u041e\u0431\u0440\u0435\u043c\u0435\u043d\u0435\u043d\u0438\u0439, \u0430\u0440\u0435\u0441\u0442\u043e\u0432 \u043d\u0435\u0442. <br><br>\u0410\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u043e \u043d\u0435\u0434\u0432\u0438\u0436\u0438\u043c\u043e\u0441\u0442\u0438 \"\u0424\u0410\u0411\u0420\u0418\u041a\u0410 \u0416\u0418\u041b\u042c\u042f\" \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0442 \u0434\u043b\u044f \u0412\u0430\u0441 \u0443\u0436\u0435 5 \u043b\u0435\u0442 \u0438 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u043e\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442 \u0443\u0441\u043b\u0443\u0433\u0438 \u043f\u043e \u043f\u043e\u043b\u043d\u043e\u043c\u0443 \u0441\u043e\u043f\u0440\u043e\u0432\u043e\u0436\u0434\u0435\u043d\u0438\u044e \u043b\u044e\u0431\u044b\u0445 \u0441\u0434\u0435\u043b\u043e\u043a \u0441 \u043d\u0435\u0434\u0432\u0438\u0436\u0438\u043c\u043e\u0441\u0442\u044c\u044e. \u041e\u043a\u0430\u0437\u0430\u043d\u0438\u0435 \u043f\u043e\u043c\u043e\u0449\u0438 \u043f\u043e \u043e\u0434\u043e\u0431\u0440\u0435\u043d\u0438\u044e \u0438\u043f\u043e\u0442\u0435\u0447\u043d\u044b\u0445 \u0437\u0430\u0439\u043c\u043e\u0432. \u0414\u043b\u044f \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 \u0410\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u0430 \u043d\u0435\u0434\u0432\u0438\u0436\u0438\u043c\u043e\u0441\u0442\u0438 \"\u0424\u0410\u0411\u0420\u0418\u041a\u0410 \u0416\u0418\u041b\u042c\u042f\" \u043d\u0430 \u0438\u043f\u043e\u0442\u0435\u0447\u043d\u044b\u0435 \u0437\u
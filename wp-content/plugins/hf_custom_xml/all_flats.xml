u043d\u043e\u0432\u0430\u044f \u0434\u043e\u0440\u043e\u0433\u0430\u044f \u0441\u0430\u043d.\u0442\u0435\u0445\u043d\u0438\u043a\u0430, \u0441\u0442\u0435\u043d\u044b \u0438 \u043f\u043e\u043b - \u0432 \u0434\u043e\u0440\u043e\u0433\u043e\u0439 \u0438\u0442\u0430\u043b\u044c\u044f\u043d\u0441\u043a\u043e\u0439 \u043f\u043b\u0438\u0442\u043a\u0435, \u0443\u043b\u043e\u0436\u0435\u043d\u043e\u0439 \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u043c \u043c\u0430\u0441\u0442\u0435\u0440\u043e\u043c.<br><br>\u041d\u0430 \u043a\u0443\u0445\u043d\u0435 \u0448\u0438\u043a\u0430\u0440\u043d\u044b\u0439 \u0438 \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0439 \u043a\u0443\u0445\u043e\u043d\u043d\u044b\u0439 \u0433\u0430\u0440\u043d\u0438\u0442\u0443\u0440 \u0441 \u043d\u0435\u043c\u0435\u0446\u043a\u043e\u0439 \u0444\u0443\u0440\u043d\u0438\u0442\u0443\u0440\u043e\u0439 \u0438 \u0434\u043e\u0432\u043e\u0434\u0447\u0438\u043a\u0430\u043c\u0438, \u0432\u044b\u043f\u043e\u043b\u043d\u0435\u043d\u043d\u0430\u044f \u0432 \u0441\u0442\u0438\u043b\u0435 \"\u0418\u0442\u0430\u043b\u044c\u044f\u043d\u0441\u043a\u0438\u0439 \u043f\u0440\u043e\u0432\u0430\u043d\u0441\". \u041a\u0443\u0445\u043e\u043d\u043d\u044b\u0439 \u0444\u0430\u0440\u0442\u0443\u043a \u0438\u0445 \u043c\u043e\u044e\u0449\u0435\u0439\u0441\u044f \u0438 \u0436\u0430\u0440\u043e\u043f\u0440\u043e\u0447\u043d\u043e\u0439 \u043f\u043b\u0438\u0442\u043a\u0438. \u041d\u0430\u0434 \u043f\u043b\u0438\u0442\u043e\u0439 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u0430 \u043f\u043e\u043b\u0443\u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u0430\u044f \u0432\u044b\u0442\u044f\u0436\u043a\u0430. \u041c\u043d\u043e\u0433\u043e\u0443\u0440\u043e\u0432\u043d\u0435\u0432\u044b\u0435 \u043d\u0430\u0442\u044f\u0436\u043d\u044b\u0435 \u043f\u043e\u0442\u043e\u043b\u043a\u0438 \u0441 \u0433\u0440\u0430\u043c\u043e\u0442\u043d\u043e \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u043e\u0432\u0430\u043d\u043d\u044b\u043c \u0440\u0435\u0448\u0435\u043d\u0438\u0435\u043c \u043f\u043e \u043e\u0441\u0432\u0435\u0449\u0435\u043d\u0438\u044e \u0432\u0438\u0437\u0443\u0430\u043b\u044c\u043d\u043e \u0434\u0435\u043b\u0430\u044e\u0442 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0443 \u0431\u043e\u043b\u044c\u0448\u0435.<br><br>\u0423\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u0430 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u0430\u044f \u044d\u043b\u0435\u043a\u0442\u0440\u043e\u043f\u0440\u043e\u0432\u043e\u0434\u043a\u0430 \u0438 \u0440\u043e\u0437\u0435\u0442\u043a\u0438, \u043f\u0430\u043a\u0435\u0442\u043d\u044b\u0439 \u0430\u0432\u0442\u043e\u043c\u0430\u0442. \u0423\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u044b \u0432\u0441\u0435 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b\u0435 \u0441\u0447\u0451\u0442\u0447\u0438\u043a\u0438, \u0447\u0442\u043e \u043f\u043e\u0437\u0432\u043e\u043b\u0438\u0442 \u043d\u043e\u0432\u044b\u043c \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u0438\u043a\u0430\u043c \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u044d\u043a\u043e\u043d\u043e\u043c\u0438\u0442\u044c \u043d\u0430 \u043a\u0432\u0430\u0440\u0442\u043f\u043b\u0430\u0442\u0435. \u0412\u043e \u0432\u0441\u0435\u0439 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0435 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0435, \u0434\u043e\u0440\u043e\u0433\u0438\u0435 \u043c\u0435\u0436\u043a\u043e\u043c\u043d\u0430\u0442\u043d\u044b\u0435 \u0434\u0432\u0435\u0440\u0438. \u041d\u0430 \u0432\u0445\u043e\u0434\u043d\u043e\u0439 \u0433\u0440\u0443\u043f\u043f\u0435 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u0430 \u0432\u0437\u043b\u043e\u043c\u043e\u0431\u0435\u0437\u043e\u043f\u0430\u0441\u043d\u0430\u044f \u0434\u0432\u0435\u0440\u044c. <br>\u0411\u043e\u043b\u044c\u0448\u0430\u044f \u043f\u0430\u043d\u043e\u0440\u0430\u043c\u043d\u0430\u044f \u043b\u043e\u0434\u0436\u0438\u044f \u0437\u0430\u0441\u0442\u0435\u043a\u043b\u0435\u043d\u0430, \u0447\u0442\u043e \u043f\u043e\u0437\u0432\u043e\u043b\u0438\u0442 \u043d\u043e\u0432\u044b\u043c \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u0438\u043a\u0430\u043c \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u043e\u0432\u0430\u0442\u044c \u043c\u0435\u0441\u0442\u043e \u0434\u043b\u044f \u0447\u0430\u0435\u043f\u0438\u0442\u0438\u044f \u0438 \u043e\u0442\u0434\u044b\u0445\u0430, \u0441 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044c\u044e \u043b\u044e\u0431\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u043f\u0440\u0435\u043a\u0440\u0430\u0441\u043d\u044b\u043c \u0432\u0438\u0434\u043e\u043c.<br><br>\u041f\u0420\u0418 \u041f\u0420\u041e\u0414\u0410\u0416\u0415 \u041e\u0421\u0422\u0410\u0401\u0422\u0421\u042f \u0432\u0441\u044f \u043c\u0435\u0431\u0435\u043b\u044c, \u0442\u0435\u0445\u043d\u0438\u043a\u0430.<br><br>\u0427\u0438\u0441\u0442\u044b\u0439 \u0438 \u0441\u043f\u043e\u043a\u043e\u0439\u043d\u044b\u0439 \u0434\u0432\u043e\u0440. \u0418\u043c\u0435\u044e\u0442\u0441\u044f \u043c\u0435\u0441\u0442\u0430 \u0434\u043b\u044f \u0442\u0440\u0430\u043d\u0441\u043f\u043e\u0440\u0442\u0430. \u0414\u0435\u0442\u0441\u043a\u0430\u044f \u043f\u043b\u043e\u0449\u0430\u0434\u043a\u0430.<br><br>\u041e\u0434\u0438\u043d \u0438\u0437 \u043b\u0443\u0447\u0448\u0438\u0445 \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u043e\u0432 \u0438\u0437 \u0438\u043c\u0435\u044e\u0449\u0438\u0445\u0441\u044f \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0435\u043d\u0438\u0439 \u0432 \u0434\u0430\u043d\u043d\u043e\u043c \u0440\u0430\u0439\u043e\u043d\u0435 \u043f\u043e \u043e\u0447\u0435\u043d\u044c \u043f\u0440\u0438\u0432\u043b\u0435\u043a\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u0446\u0435\u043d\u0435.<br>\u041f\u043e\u0434\u0445\u043e\u0434\u0438\u0442 \u043f\u043e\u0434 \u0438\u043f\u043e\u0442\u0435\u043a\u0443.<br><br>\u0418\u043d\u0444\u0440\u0430\u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0430: \u0440\u044f\u0434\u043e\u043c \u0440\u0430\u0441\u043f\u043e\u043b\u043e\u0436\u0435\u043d\u044b: \u0448\u043a\u043e\u043b\u0430, \u0434\u0435\u0442\u0441\u043a\u0438\u0439 \u0441\u0430\u0434, \u0441\u0435\u0442\u0435\u0432\u044b\u0435 \u043c\u0430\u0433\u0430\u0437\u0438\u043d\u044b \u041c\u0430\u0433\u043d\u0438\u0442, \u041f\u044f\u0442\u0451\u0440\u043e\u0447\u043a\u0430, \u041f\u043e\u043a\u0443\u043f\u043e\u0447\u043a\u0430, \u0445\u0438\u043c\u0447\u0438\u0441\u0442\u043a\u0430 \u0417\u043e\u043b\u0443\u0448\u043a\u0430, \u0430\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u043e \u0410\u044d\u0440\u043e\u0444\u043b\u043e\u0442, \u0414\u0435\u0442\u0441\u043a\u0438\u0439 \u0426\u0435\u043d\u0442\u0440 \u0438 \u043f\u0440\u043e\u0447\u0438\u0435 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u043d\u044b\u0435 \u0438 \u0434\u043e\u0441\u0443\u0433\u043e\u0432\u044b\u0435 \u043c\u0435\u0441\u0442\u0430.<br><br>\u0412\u0431\u043b\u0438\u0437\u0438 \u0440\u0430\u0441\u043f\u043e\u043b\u043e\u0436\u0435\u043d\u044b \u0443\u043b\u0438\u0446\u044b: \u043f\u0440. \u0416\u0443\u043a\u043e\u0432\u0430, \u041a\u0430\u0440\u043b\u0430 \u041b\u0438\u0431\u043a\u043d\u0435\u0445\u0442\u0430, \u041a\u0430\u0447\u0438\u043d\u0446\u0435\u0432, \u0425\u043e\u0440\u043e\u0448\u0435\u0432\u0430, \u0418\u0441\u0442\u0440\u0438\u043d\u0441\u043a\u0430\u044f.<br>\u0412 \u0448\u0430\u0433\u043e\u0432\u043e\u0439 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u043e\u0441\u0442\u0438 \u0422\u0420\u0426 \u041f\u0430\u0440\u043a\u0425\u0430\u0443\u0437. 10 \u043c\u0438\u043d\u0443\u0442 \u043d\u0430 \u0430\u0432\u0442\u043e \u0434\u043e \u0433\u043b\u0430\u0432\u043d\u043e\u0439 \u0432\u044b\u0441\u043e\u0442\u044b \u0420\u043e\u0441\u0441\u0438\u0438 - \u041c\u0430\u043c\u0430\u0435\u0432\u0430 \u043a\u0443\u0440\u0433\u0430\u043d\u0430 \u0441 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u043d\u044b\u043c \u043d\u0430 \u043d\u0451\u043c \u043c\u043e\u043d\u0443\u043c\u0435\u043d\u0442\u043e\u043c \"\u0420\u043e\u0434\u0438\u043d\u0430-\u041c\u0430\u0442\u044c\". <br><br>\u0422\u0440\u0430\u043d\u0441\u043f\u043e\u0440\u0442\u043d\u0430\u044f \u0440\u0430\u0437\u0432\u044f\u0437\u043a\u0430 \u0441 \u043c\u043d\u043e\u0433\u043e\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u044b\u043c\u0438 \u043c\u0430\u0440\u0448\u0440\u0443\u0442\u043d\u044b\u043c\u0438 \u0442\u0430\u043a\u0441\u0438, \u0442\u0440\u0430\u043c\u0432\u0430\u044f\u043c\u0438, \u0430\u0432\u0442\u043e\u0431\u0443\u0441\u0430\u043c\u0438, \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u0435\u0442 \u0431\u0435\u0437 \u0437\u0430\u0442\u0440\u0443\u0434\u043d\u0435\u043d\u0438\u0439 \u043f\u043e\u043f\u0430\u0441\u0442\u044c \u0432 \u043b\u044e\u0431\u0443\u044e \u0447\u0430\u0441\u0442\u044c \u0433\u043e\u0440\u043e\u0434\u0430-\u0433\u0435\u0440\u043e\u044f \u0412\u043e\u043b\u0433\u043e\u0433\u0440\u0430\u0434\u0430.<br><br>\u0423\u0421\u041b\u041e\u0412\u0418\u042f \u041f\u0420\u041e\u0414\u0410\u0416\u0418: \u0447\u0438\u0441\u0442\u0430\u044f \u043f\u0440\u043e\u0434\u0430\u0436\u0430, \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b \u043a \u0441\u0434\u0435\u043b\u043a\u0435 \u0433\u043e\u0442\u043e\u0432\u044b.<br><br>\u0410\u0433\u0435\u043d\u0441\u0442\u0432\u043e \u043d\u0435\u0434\u0432\u0438\u0436\u0438\u043c\u043e\u0441\u0442\u0438 \"\u0424\u0410\u0411\u0420\u0418\u041a\u0410 \u0416\u0418\u041b\u042c\u042f\" \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0442 \u0434\u043b\u044f \u0412\u0430\u0441 \u0443\u0436\u0435 5 \u043b\u0435\u0442 \u0438 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u043e\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442 \u0443\u0441\u043b\u0443\u0433\u0438 \u043f\u043e \u043f\u043e\u043b\u043d\u043e\u043c\u0443 \u0441\u043e\u043f\u0440\u043e\u0432\u043e\u0436\u0434\u0435\u043d\u0438\u044e \u043b\u044e\u0431\u044b\u0445 \u0441\u0434\u0435\u043b\u043e\u043a \u0441 \u043d\u0435\u0434\u0432\u0438\u0436\u0438\u043c\u043e\u0441\u0442\u044c\u044e. \u041e\u043a\u0430\u0437\u0430\u043d\u0438\u0435 \u043f\u043e\u043c\u043e\u0449\u0438 \u043f\u043e \u043e\u0434\u043e\u0431\u0440\u0435\u043d\u0438\u044e \u0438\u043f\u043e\u0442\u0435\u0447\u043d\u044b\u0445 \u0437\u0430\u0439\u043c\u043e\u0432. \u0414\u043b\u044f \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 \u0410\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u0430 \u043d\u0435\u0434\u0432\u0438\u0436\u0438\u043c\u043e\u0441\u0442\u0438 \"\u0424\u0410\u0411\u0420\u0418\u041a\u0410 \u0416\u0418\u041b\u042c\u042f\" \u043d\u0430 \u0438\u043f\u043e\u0442\u0435\u0447\u043d\u044b\u0435 \u0437\u0430\u0439\u043c\u044b \u043e\u0442 \u041f\u0410\u041e \u0421\u0431\u0435\u0440\u0411\u0430\u043d\u043a \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0441\u043a\u0438\u0434\u043a\u0430 \u0432 \u0440\u0430\u0437\u043c\u0435\u0440\u0435 0.3%. <br><br>\u041c\u044b \u0437\u0430\u0431\u043e\u0442\u0438\u043c\u0441\u044f \u043e \u0412\u0430\u0448\u0435\u043c \u043a\u043e\u043c\u0444\u043e\u0440\u0442\u0435! <br><br>\u0415\u0441\u043b\u0438 \u044d\u0442\u0430 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0430 \u043d\u0435 \u0432 \u043f\u043e\u043b\u043d\u043e\u0439 \u043c\u0435\u0440\u0435 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \u0412\u0430\u0448\u0438\u043c \u0437\u0430\u043f\u0440\u043e\u0441\u0430\u043c, \u0412\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u043f\u043e\u0434\u043e\u0431\u0440\u0430\u0442\u044c \u0441\u0432\u043e\u0435 \u0436\u0438\u043b\u044c\u0451 \u0432 \u043c\u0430\u0433\u0430\u0437\u0438\u043d\u0435 \u0410\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u0430 \u043d\u0435\u0434\u0432\u0438\u0436\u0438\u043c\u043e\u0441\u0442\u0438 \"\u0424\u0410\u0411\u0420\u0418\u041a\u0410 \u0416\u0418\u041b\u042c\u042f\" \u043f\u043e\u0434 \u044d\u0442\u0438\u043c \u04